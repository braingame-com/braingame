{
	"rules": [
		{
			"id": "@secretlint/secretlint-rule-preset-recommend"
		},
		{
			"id": "@secretlint/secretlint-rule-no-dotenv"
		}
	],
	"allowMessageIds": [
		"@secretlint/secretlint-rule-preset-recommend>@secretlint/secretlint-rule-aws>AWS_ACCESS_KEY_ID",
		"@secretlint/secretlint-rule-preset-recommend>@secretlint/secretlint-rule-gcp>GCP_SERVICE_ACCOUNT",
		"@secretlint/secretlint-rule-preset-recommend>@secretlint/secretlint-rule-github>GITHUB_TOKEN"
	],
	"options": {
		"severity": "error"
	}
}
