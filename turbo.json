{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**", "web-build/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"test": {
			"dependsOn": ["^test"],
			"outputs": ["coverage/**"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"clean": {
			"cache": false
		}
	}
}
